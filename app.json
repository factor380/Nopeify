{
  "expo": {
    "name": "Nopeify",
    "slug": "hello-world",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/NopeifyIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "nopeify",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.binyaminfactor380.Nopeify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_DATA_SYNC",
        "WAKE_LOCK"
      ],
      "config": {
        "foregroundServiceType": "dataSync"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.binyaminfactor380.Nopeify"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b8b52f08-deb6-44e1-ad53-e87a0e1e4e7a"
      }
    },
    "plugins": [
      "expo-notifications",
      "./withBackgroundActions.js"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b8b52f08-deb6-44e1-ad53-e87a0e1e4e7a"
    }
  }
}
